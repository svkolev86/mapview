<UserControl x:Class="ROH.Web.Client.Silverlight.DeepZoomContainer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="480" d:DesignWidth="480">
    <ScrollViewer x:Name="LayoutRoot" Background="White" VerticalScrollBarVisibility="Disabled" BorderThickness="0,0,0,0" Padding="0,0,0,0" UseLayoutRounding="False">
        <Grid x:Name="scrBase">
            <MultiScaleImage x:Name="msiMap" d:IsHidden="True" Margin="0,0,0,0" Grid.RowSpan="1" Loaded="image_Loaded"/>
            <Grid x:Name="grdLayer1"/>
        </Grid>
    </ScrollViewer>
</UserControl>
